        "vue": "^2.5.7",
        "vue-resource": "^1.3.4"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "gentelella": "^1.4.0",
        "ti-icons": "^0.1.2",
        "vue-form-wizard": "^0.8.4",
        "vue-good-table": "^2.16.0",
        "vuejs-inputmask": "0.0.8"
    }
